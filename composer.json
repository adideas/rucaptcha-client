{
  "name": "gladyshev/rucaptcha-client",
  "keywords": ["captcha recognition", "rucaptcha", "2captcha.com", "rucaptcha.com"],
  "description": "Lightweight and useful 2captcha.com/rucaptcha.com PHP-client with PSRs support.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitry Gladyshev",
      "email": "deel@email.ru"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "ext-json": "*",
    "ext-mbstring": "*",
    "psr/http-client": "~1.0.0",
    "psr/log": "~1.0.0",
    "guzzlehttp/guzzle": "~7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Rucaptcha\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rucaptcha\\Test\\": "test/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit"
  }
}
